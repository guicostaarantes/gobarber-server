{
  "name": "gobarber-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:start": "env-cmd -f development.env ts-node-dev --inspect --transpileOnly --ignore-watch node_modules src/shared/infra/http/server.ts",
    "dev:typeorm": "env-cmd -f development.env ts-node-dev ./node_modules/typeorm/cli.js",
    "test": "env-cmd -f test.env jest",
    "test:coverage": "env-cmd -f test.env jest --coverage",
    "staging:build": "env-cmd -f staging.env tsc",
    "production:build": "env-cmd -f production.env tsc"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.12.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.2.0",
    "typeorm": "^0.2.24",
    "uuidv4": "^6.0.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "4.17.3",
    "@types/express-serve-static-core": "4.17.2",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/multer": "^1.4.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^25.3.1",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
